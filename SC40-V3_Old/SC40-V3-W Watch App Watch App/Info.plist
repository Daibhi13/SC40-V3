<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Sprint Coach</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>CFBundleName</key>
	<string>Sprint Coach Watch</string>
	<key>CFBundlePackageType</key>
	<string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
	<key>CFBundleIcons</key>
	<dict>
		<key>CFBundlePrimaryIcon</key>
		<dict>
			<key>CFBundleIconFiles</key>
			<array>
				<string>AppIcon Watch</string>
			</array>
			<key>CFBundleIconName</key>
			<string>AppIcon Watch</string>
		</dict>
	</dict>
	<key>CFBundleIconFile</key>
	<string>AppIcon Watch</string>
	<key>CFBundleIconFiles</key>
	<array>
		<string>AppIcon Watch</string>
	</array>
	<key>NSHealthShareUsageDescription</key>
	<string>Sprint Coach reads your workout data to track progress and sync with your iPhone.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>Sprint Coach saves your sprint workouts and heart rate data to the Health app.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Sprint Coach tracks your location during workouts to provide accurate sprint timing and performance metrics.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Sprint Coach uses GPS to accurately track your sprint times and distances during workouts.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Sprint Coach can use the microphone for voice commands during outdoor workouts.</string>
	<key>NSMotionUsageDescription</key>
	<string>Sprint Coach uses motion data to enhance workout tracking accuracy.</string>
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>Sprint Coach can recognize voice commands for hands-free workout control.</string>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>UIBackgroundModes</key>
	<array>
		<string>workout-processing</string>
		<string>location</string>
		<string>background-audio</string>
		<string>remote-notification</string>
	</array>
	<key>UIPrerenderedIcon</key>
	<false/>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
	</array>
	<key>UIUserInterfaceStyle</key>
	<string>Automatic</string>
