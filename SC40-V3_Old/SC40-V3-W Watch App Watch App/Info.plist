	<true/>
	<key>WKSupportsProMotion</key>
	<true/>
	<key>WKSupportsTheaterMode</key>
	<true/>
	<key>WKSupportsVariableRefreshRate</key>
	<true/>
	<key>WKSupportsVoiceOver</key>
	<true/>
	<key>WKSupportsWaterLock</key>
	<true/>
